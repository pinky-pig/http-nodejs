{
  "version": 2,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node",
      "env": {
        "API_URL": "https://dev.mmeme.me/"
      },
      "command": "npm run build"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server.mjs" }
  ]
}
